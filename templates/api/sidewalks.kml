<?xml version="1.0" encoding="utf-8" ?>
<kml xmlns="http://www.opengis.net/kml/2.2">
<Document><Folder><name>UCF Campus Map Sidewalks</name>

{% for s in sidewalks %}{% if s.kml_coords %}
<Placemark>
	<Style><LineStyle><color>999a6700</color><width>8</width></LineStyle><PolyStyle><fill>0</fill></PolyStyle></Style>
	<LineString><coordinates>{{s.kml_coords}}</coordinates></LineString>
</Placemark>
{%endif%}{%endfor %}

</Folder></Document></kml>